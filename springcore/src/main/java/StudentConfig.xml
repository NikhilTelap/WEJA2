<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean class="com.jspiders.springcore.bean.StudentBean">
<property name="id">
<value>1</value>
</property>
<property name="name" value="nikhil"/>
<property name="email" value="nikhil@gmail.com"/>
</bean>

<bean class="com.jspiders.springcore.bean.StudentBean" name="student2">
<constructor-arg name="id">
<value>2</value>
</constructor-arg>
<constructor-arg name="name" value="vivek"/>
<constructor-arg name="email" value="vivek@gmail.com"/>

</bean>

</beans>